server.port=8080

logging.config=classpath:logback-spring.xml

#server.servlet.context-path=/es-boot
spring.data.elasticsearch.cluster-nodes=192.168.0.10:9200

#自定义属性
basefiledir=dictionary

spring.datasource.url = jdbc:mysql://192.168.0.6:3306/test?useSSL=false&useUnicode=true&characterEncoding=UTF-8&ENGINE=InnoDB
spring.datasource.username = knowledge
spring.datasource.password = mysql
spring.datasource.driverClassName = com.mysql.jdbc.Driver
spring.datasource.max-active=20
spring.datasource.max-idle=8
spring.datasource.min-idle=8
spring.datasource.initial-size=10


spring.jpa.database = MYSQL
# Show or not log for each sql query
spring.jpa.show-sql = true
# Hibernate ddl auto (create, create-drop, update,validate) validate 检测到不一致不做任何操作
spring.jpa.hibernate.ddl-auto = update
# Naming strategy
spring.jpa.hibernate.naming-strategy = org.hibernate.cfg.ImprovedNamingStrategy
# stripped before adding them to the entity manager)
#不加这句则默认为myisam引擎
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL5InnoDBDialect

spring.application.name=knowledge-dictionary
#server.port=8762
eureka.client.serviceUrl.defaultZone=http://localhost:8761/eureka/





